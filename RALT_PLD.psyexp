ght_color = &quot;white&quot;;&amp;#10;    } else {&amp;#10;        if ((response_training.keys == null)) {&amp;#10;            left_color = &quot;grey&quot;;&amp;#10;            right_color = &quot;grey&quot;;&amp;#10;            time_miss = 6;&amp;#10;            time_a = 0;&amp;#10;            time_h = 0;&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
